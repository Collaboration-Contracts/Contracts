<!DOCTYPE html>
<html>
  <head>
    <title>Cont</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h1>Collaboration Contracts</h1>
    <% flash.each do |k,v| %>
      <p class="<%= k %> flash_message"><span class="excl">&#10071;</span> <%= v %></p>
    <% end %>
    <%= yield %>
  </body>
</html>
